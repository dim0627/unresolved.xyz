{
  "name": "unresolved.xyz",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "biome check ./apps ./packages",
    "lint:fix": "biome check ./apps ./packages --write --unsafe",
    "syncpack:fix": "syncpack fix",
    "syncpack:format": "syncpack format",
    "syncpack:lint": "syncpack lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "syncpack": "14.0.0-alpha.19",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.10.0"
  },
  "packageManager": "pnpm@10.12.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
