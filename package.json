{
  "name": "unresolved.xyz",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "biome check ./apps ./packages && markdownlint-cli2",
    "lint:fix": "biome check ./apps ./packages --write --unsafe && markdownlint-cli2 --fix",
    "syncpack:fix": "syncpack fix",
    "syncpack:format": "syncpack format",
    "syncpack:lint": "syncpack lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "markdownlint-cli2": "^0.20.0",
    "syncpack": "14.0.0-alpha.41",
    "turbo": "^2.8.3"
  },
  "engines": {
    "node": ">=18.10.0"
  },
  "packageManager": "pnpm@10.12.2",
  "pnpm": {
    "overrides": {
      "axios": "1.13.5"
    }
  }
}
